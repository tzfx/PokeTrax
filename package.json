    "xhr2": "^0.2.1"
  },
  "scripts": {
    "start": "export BROWSER=none && vite",
    "start-win": "set BROWSER=none && vite",
    "start-electron": "export ELECTRON_START_URL=http://localhost:3000 && export NODE_ENV=development && electron .",
    "start-electron-win": "set ELECTRON_START_URL=http://localhost:3000 && set NODE_ENV=development && electron .",
    "start-test-server": "export NODE_ENV=development && node src/server/test-server.js",
    "build": "vite build",
    "test": "export NODE_ENV=test && mocha --timeout 60000",
    "test-cypress": "npx cypress run",
    "make": "vite build && electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^9.6.1",
    "electron": "^17.4.0",
    "electron-builder": "^23.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "mocha": "^10.0.0",
    "postcss": "^8.4.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.0.1",
    "typescript": "^4.6.3"
  },
  "build": {
    "appId": "github.com/jgunzelman88/PokeTrax",
    "extraFiles": {
      "from": "./dist",
      "to": "./"
    },
    "dmg": {
      "icon": "icon.icns",
      "artifactName": "poketrax.dmg"
    },
    "win": {
      "artifactName": "poketrax.exe"
    },
    "snap":{
      "artifactName": "poketrax.snap"
    },
    "directories": {
      "buildResources": "buildResources",
      "output": "build"
    }
  }
}
